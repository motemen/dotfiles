highlight CmdLine font-size: 12pt; padding: 5px; z-index: 9999; background-color: rgba(255, 255, 255, 0.8); transition: background-color: 2s;
highlight InfoMsg font-size: 12pt; color: green;
highlight CmdLine>* font-family: "Source Code Pro", Menlo, monospace;
highlight StatusLine font-size: 10pt; font-family: "Avenir Next", "Lucida Grande", sans-serif;
highlight CmdOutput font-size: 12pt;
highlight CompItem font-size: 12pt; font-family: "Source Code Pro", Menlo, monospace;
highlight CompTitle font-size: 12pt; font-weight: bold;
highlight CompTitle>* text-shadow: none; padding: 3px;
